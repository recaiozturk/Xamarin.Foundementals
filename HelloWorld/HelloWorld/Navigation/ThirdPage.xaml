<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="HelloWorld.Navigation.ThirdPage"
             Title="Third Page">
   

        <StackLayout HorizontalOptions="Center" 
                     VerticalOptions="Center">
            <Label Text="Third Page" HorizontalOptions="Center"/>
        <Button x:Name="PrevPageButton"
                Text="Previous Page"
                Clicked="PrevPageButton_Clicked"/>

        <Button x:Name="FirstPageButton"
                Text="First Page"
                Clicked="FirstPageButton_Clicked"/>
    </StackLayout>

</ContentPage>